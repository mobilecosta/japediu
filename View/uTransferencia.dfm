  object qryEmpresa: TFDQuery
    Connection = Dados.Conexao
    SQL.Strings = (
      'SELECT CODIGO,FANTASIA FROM('
      'SELECT E.codigo, E.fantasia FROM EMPRESA E'
      'UNION '
      
        'SELECT (0) AS CODIGO, ('#39'<todas as empresas>'#39') AS FANTASIA from R' +
        'DB$DataBase'
      ')'
      'where'
      'CODIGO>=0'
      'order by FANTASIA')
    Left = 231
    Top = 16
